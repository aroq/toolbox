#!/usr/bin/env variant
# vi: ft=yaml

import: github.com/aroq/toolbox?ref=v2.1//variant-lib/utils.yaml

description: Uniconf tasks

parameters:
- name: TOOLBOX_UNICONF_CONFIG_FILE
  default: config/defines.yaml

steps:
- task: exec
  arguments:
    cmd: |
      uniconf -c {{ .TOOLBOX_UNICONF_CONFIG_FILE }}
